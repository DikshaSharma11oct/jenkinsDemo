pipeline{
    agent any

    stages{
        stage('compile'){
            steps{
                bat 'javac Student.java'
            }
        }
        stage('run'){
            steps{
                bat 'java Student'
            }
        }
    }
    post{
        always{
            echo 'always print a message'

        }
        success{
            echo 'success msg'

        }
        failure{
            echo 'failure msg'
        }
    }
}
